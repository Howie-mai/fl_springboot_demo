        <commons-logging.version>1.1.1</commons-logging.version>
        <commons-logging-api.version>1.1</commons-logging-api.version>
        <commons-pool.version>1.5.6</commons-pool.version>
        <!-- commons -->
        <dom4j.version>1.6.1</dom4j.version>
